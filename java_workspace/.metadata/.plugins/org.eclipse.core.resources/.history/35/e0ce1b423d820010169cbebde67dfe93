package day01;

public class HelloWorld {

	public static void main(String[] args) {
		// class는 package 안에 있어야함
		// class는 대문자로 시작(공백 X)
		// class 명에 여러 단어 사용 시 카멜케이스 사용
		// HelloWorld
		// 대소문자 구분 HelloWorld != Helloworld
		// -> 주석 : 프로그램으로 읽히지 않는 라인 // : 한줄 주석 /**/ : 사이 내용 전부 주석
		// ctrl + / : 주석
		/*
		 * println(); : 줄바꿈 있는 출력
		 * print(); : 줄바꿈 없는 출력
		 * printf(); : 포멧형 출력
		 */
		System.out.println("Hello Java");
		System.out.print("Hello Java World~~!");

	}

}
