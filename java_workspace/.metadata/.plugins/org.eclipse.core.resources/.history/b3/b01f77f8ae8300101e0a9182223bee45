package day03;

public class For03 {

  public static void main(String[] args) {
    /* 구구단 2단 출력
     * 2*1=2
     * 2*2=4
     * 2*3=6
     * ---
     * 2*9=18
     */
    
    int dan = 2;
    int calc = 1;
    for(int i = 1; i<10 ; i++ ) {
      calc = dan * i;
      System.out.println("2*"+i+"="+dan);
    }

  }

}
