package day01;

public class Operator {

	public static void main(String[] args) {
		// 연산자(Operator)
		// 사칙연산자 ( + - * / %) : () 우선순위
		// 대입연산자 (= += -=)
		// 왼쪽은 반드시 변수
		
		int a = 1;
		int b = 2;
		//a = b; // 1은 사라지고 b의 2값이 a로 저장
		// 누적
		a += b; // a = a + b;
		System.out.println(a);
		
		

	}

}
